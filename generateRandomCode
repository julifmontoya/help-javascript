    generateCode() {
      const characters = "ABCDEFGHIJKLMNOPQRSTUVWXYZ123456789";
      const uniqueId = () => {
        let text = "";
        for (let i = 0; i < 8; i++) {
          text += characters.charAt(
            Math.floor(Math.random() * characters.length)
          );
        }
        return text;
      };
      return uniqueId();
    },
